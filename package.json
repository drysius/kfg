{
  "name": "kfg",
  "type": "module",
  "description": "Simple system of standardization of configurations for node js and bun.",
  "version": "1.1.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drysius/kfg.git"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "backend",
    "standardization",
    "standard",
    "env",
    "config-js",
    "json",
    "kfg",
    "kfgfs",
    "kfg-fs",
    "kfg-js",
    "config",
    "configjs",
    "npm",
    "node",
    "bun",
    "deno",
    "typescript",
    "debug",
    "output",
    "performance"
  ],
  "author": "Drysius",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drysius/kfg/issues"
  },
  "homepage": "https://kfg.js.org",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.test.json && bun run build.ts",
    "docs": "typedoc",
    "test": "bun test",
    "check": "biome check --write",
    "prebuild": "tsc --project tsconfig.test.json"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/bun": "^1.2.21",
    "@types/node": "^24.3.0",
    "dts-bundle-generator": "^9.5.1",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.14",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.41"
  }
}
